<RoundedButton@Button>:
    background_color:(0,0,0,0)
    background_normal: ""
    canvas.before:
        Color:
            rgba:(0,0.65,0.65,1)
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [25]    